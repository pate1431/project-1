pipeline{
  agent any
  stages{
    stage("build"){
      steps{
        sh "echo Jay Shree Ram using Jenkins File"
        sh "echo Jay siya ram"
      }
    }
    stage("installation"){
      steps{
        sh "ls"
        sh "pwd"
        sh "touch simple.txt"
        sh "echo helo world >> simple.txt"
        sh "echo asdhiaoh >> simple.txt"
      }
    }
  }
}
